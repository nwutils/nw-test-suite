{
  "name": "nw-api-sanity-check",
  "main": "index.html",
  "private": true,
  "scripts": {
    "ci:fmt": "prettier --check \"./**/*.{css,html,js,json,md,yml}\"",
    "ci:lnt": "eslint --config=.github/eslint.config.cjs --ext \"./**/*.js\"",
    "fmt": "prettier --write \"./**/*.{css,html,js,json,md,yml}\"",
    "lnt": "eslint --config=.github/eslint.config.cjs --ext \"./**/*.js\"",
    "manual": "nw .",
    "start": "node --test-reporter=spec --test chrome nw"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "nw": "0.79.0-sdk",
    "prettier": "^3.0.3",
    "selenium-webdriver": "^4.11.1"
  },
  "engines": {
    "node": "v20.5.0",
    "npm": "9.8.1"
  },
  "packageManager": "npm@9.8.1"
}
